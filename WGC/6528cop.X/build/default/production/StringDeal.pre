bool FindStringHead(struct StringCmpResult* scr_input) {
uint8_t postion;
for (postion = scr_input->cmpPzt; postion < scr_input->inputlgt; postion++) {
if (*(scr_input->inputstring + postion) == *scr_input->cmpstring) {
