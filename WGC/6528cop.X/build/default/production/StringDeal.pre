}

bool CmpString(struct StringCmpResult* scr_input) {
uint8_t cnt;
for (cnt = 0; cnt < scr_input->cmplgt; cnt++) {
if (*(scr_input->cmpstring + cnt) != *(scr_input->inputstring + cnt + scr_input->cmpPzt))
break;
else if (cnt == scr_input->cmplgt - 1)
return 1;
}
scr_input->cmpPzt += 1;
return 0;
}

